<%= ERB.new(File.read("#{ENV['APP_APP_DIR']}/config/src/logstash-common-filters.conf.erb")).result(binding) %>

input {
    stdin {
        type => "<%= @args[:logstash_type] %>"
    }
}

output {
    #stdout { debug => true debug_format => "json"}
    elasticsearch_http {
        flush_size => 100
    }
}
